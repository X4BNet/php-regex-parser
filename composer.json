{
    "name": "robinbressan/regex-parser",
    "description": "AST for PCRE Regex",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Bressan",
            "email": "robin@buddey.net"
        }
    ],
    "require-dev": {
        "ext-dom": "*",
        "ext-json": "*",
        "jangregor/phpstan-prophecy": "^1.0",
        "phpspec/prophecy": "^1.17",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9 || ^10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "RegexParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RegexParser\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "config": {
        "sort-packages": true
    }
}
